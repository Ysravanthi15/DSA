import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int T=sc.nextInt();
		while(T-->0){
		    int x=sc.nextInt();
		    if(x<300){
		        System.out.println(300*10);
		    }
		    else{
		        System.out.println(x*10);
		    }
		    
		}

	}
}

